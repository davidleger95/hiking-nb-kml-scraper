      <name>Waypoint / Repères</name>
      <Placemark>
        <name>Parking / Stationnement</name>
        <styleUrl>#icon-1644-E65100-nodesc</styleUrl>
        <Point>
          <coordinates>
            -66.1011267,45.2702805,0
